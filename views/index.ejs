<%- include('partials/header.ejs') %>


    <div class="cardContainer">
        
        <div class="card">
            <% if (locals.errorMessage) { %>
            <p class="errorMessage"><%= errorMessage %></p>
            <% } else if (locals.messageButton) {%>
                <p class="errorMessage"><%= messageButton.message %></p>
                <%- messageButton.button %>
                <% } %>
            <% if (locals.allData) { %>
            <form action="/get-weather" id="form" class="form" method="post">
                <div class="select">
                    <label for="country" class="countryText">Input name of City or Contry</label>
                    <div class="topnav">
                        
                        <input type="text" id="country" name="country" placeholder="Search..">
                        <input id="getWeather" type="submit" value="Submit" formaction="/get-weather">

                    </div>
                </div>
            </form>
            
          <p class="city"><%= allData.fullName %></p>
          

          
          <p class="weather"><%= allData.statusDescription %></p>
          
          <svg
            class="weather"
            version="1.1"
            id="Layer_1"
            x="0px"
            y="0px"
            width="50px"
            height="50px"
            viewBox="0 0 100 100"
            xml:space="preserve"
          >
            <image
              id="image0"
              width="100"
              height="100"
              x="0"
              y="0"
              href="data:image/png;base64,"
            ></image>
          </svg>
          
          
          <p class="temp"><%= allData.temperature %> °</p>
          
          
          <div class="weatherDataContainer">
            <div class="mainWeatherContainer">

                <div class="first">
                    <p class="minHeading">Min Temp</p>
                    
                    <p class="minTemp"><%= allData.minTemperature %>°</p>
                    
                </div>

                <div class="new">
                    <p class="minHeading">Humidity</p>
                    
                    <p class="minTemp"><%= allData.humidity %></p>
                    
                </div>

                <div class="new">
                    <p class="minHeading">Wind Speed</p>
                    
                    <p class="minTemp"><%= allData.windSpeed %></p>
                    
                </div>

                <div class="last">
                    <p class="minHeading">Max Temp</p>
                    
                    <p class="minTemp"><%= allData.maxTemperature %>°</p>
                    
                </div>
            </div>
          </div>
        </div>
    </div>
      
</form>
<% } %>

<div class="buttons-year">   
    <div class="buttons-container">
            <div class="outline">
                <span class="top-key"></span>
                <a href="https://www.marius-bogdan.com/">
                    <button class="return-button">
                        <div class="return-sign"><svg viewBox="0 0 512 512"><path d="M377.9 105.9L500.7 228.7c7.2 7.2 11.3 17.1 11.3 27.3s-4.1 20.1-11.3 27.3L377.9 406.1c-6.4 6.4-15 9.9-24 9.9c-18.7 0-33.9-15.2-33.9-33.9l0-62.1-128 0c-17.7 0-32-14.3-32-32l0-64c0-17.7 14.3-32 32-32l128 0 0-62.1c0-18.7 15.2-33.9 33.9-33.9c9 0 17.6 3.6 24 9.9zM160 96L96 96c-17.7 0-32 14.3-32 32l0 256c0 17.7 14.3 32 32 32l64 0c17.7 0 32 14.3 32 32s-14.3 32-32 32l-64 0c-53 0-96-43-96-96L0 128C0 75 43 32 96 32l64 0c17.7 0 32 14.3 32 32s-14.3 32-32 32z"></path></svg></div>
                        <div class="return-text">Return</div>
                    </button>
                </a>
                <a href="https://github.com/MIBogdan/projects/tree/main">
                    <button class="code-button">
                        <span class="bracket left">&#123;</span>
                        <span class="code-text">View Code</span>
                        <span class="bracket right">&#125;</span>
                    </button>
                </a>
                <span class="bottom-key-1"></span>
                <span class="bottom-key-2"></span>
